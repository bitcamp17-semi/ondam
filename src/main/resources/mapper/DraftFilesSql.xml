<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.DraftFilesMapper">
    <insert id="createFiles" parameterType="DraftFilesDto">
        insert into draft_files (name, draftId, path, createdAt)
        values (#{name}, #{draftId}, #{path}, now())
    </insert>
</mapper>